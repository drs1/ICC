class Fibonacci {

    void main(string[] args) {
        string s = args[0];
        int n,r;

        n = Library.stoi(s,-1);
        r = fib(n);
        
        Library.printi(r);
        Library.println("");
    }

    int fib(int n) {
        if (n < 2) return n;
        return fib(n-1) + fib(n-2);
    } 
}

